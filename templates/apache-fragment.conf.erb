<% unless @suburi %>
PassengerAppRoot <%= @app_root %>
<% end %>
<% if @ruby and !@ruby.empty? -%>
PassengerRuby <%= @ruby %>
<% end -%>
<%= "PassengerMinInstances #{@min_instances}\n" unless @min_instances == '0' -%>
<%= "PassengerStartTimeout #{@start_timeout}\n" unless @start_timeout == '0' -%>

AddDefaultCharset UTF-8
